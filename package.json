{
  "name": "nvm-web-interface",
  "version": "1.0.0",
  "description": "Web-based interface for Node Version Manager (NVM)",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "child_process": "^1.0.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "nvm",
    "node",
    "version",
    "manager",
    "web",
    "interface"
  ],
  "author": "NVM Web Interface",
  "license": "MIT"
}
